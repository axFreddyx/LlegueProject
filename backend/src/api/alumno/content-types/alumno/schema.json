{
  "kind": "collectionType",
  "collectionName": "alumnos",
  "info": {
    "singularName": "alumno",
    "pluralName": "alumnos",
    "displayName": "Alumno"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "Apellidos": {
      "type": "string",
      "required": true
    },
    "foto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "salon": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::salon.salon",
      "inversedBy": "alumnos"
    },
    "persona_autorizada": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "alumnos"
    }
  }
}
